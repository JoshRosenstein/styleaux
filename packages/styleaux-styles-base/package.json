{
  "name": "@styleaux/styles-base",
  "version": "0.0.0-beta.0",
  "main": "dist/index.js",
  "publishConfig": {
    "access": "public"
  },
  "sideEffects": "false",
  "files": [
    "dist"
  ],
  "author": "Joshua Rosenstein",
  "license": "MIT",
  "dependencies": {
    "@styleaux/core": "^1.9.0-beta.0",
    "@roseys/futils": "^2.1.0",
    "typed-is": "^1.0.4"
  },
  "scripts": {
    "commit": "git-cz",
    "build": "yarn clean && yarn build:es && yarn build:cjs",
    "build:browser": "echo 'Webpack needs to be configured with babel loader'",
    "build:node": "tsc",
    "build:es": "tsc --declaration true --declarationMap true --module esnext --outDir ./dist/es",
    "build:cjs": "tsc --declaration true --declarationMap true --module commonjs --outDir ./dist/commonjs",
    "clean": "rimraf dist && rimraf .tmp",
    "dist": "yarn clean && yarn build",
    "test": "jest"
  },
  "devDependencies": {
    "@johanneslumpe/css-types": "^0.3.0",
    "typescript": "^3.3.3333",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.3",
    "cross-env": "^5.2.0",
    "csstype": "^2.6.2",
    "jest": "^24.5.0",
    "jest-cli": "^23.6.0",
    "rimraf": "^2.6.3",
    "ts-jest": "^24.0.0",
    "tsargs": "^1.3.1",
    "uglifyjs-webpack-plugin": "^2.1.1",
    "webpack": "^4.29.5",
    "webpack-cli": "^3.2.3"
  }
}
