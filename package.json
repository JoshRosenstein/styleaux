{
  "name": "styleaux",
  "private": true,
  "author": "Joshua Rosenstein",
  "devDependencies": {
    "@babel/core": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-syntax-import-meta": "^7.2.0",
    "@babel/preset-env": "^7.4.1",
    "@babel/preset-react": "^7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@roseys/jest-preset": "^1.0.3",
    "@types/jest": "^24.0.11",
    "@types/node": "^11.11.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.5.0",
    "babel-plugin-dynamic-import-node-babel-7": "^2.0.7",
    "builtin-modules": "^3.0.0",
    "bundlesize": "^0.17.0",
    "codecov": "^3.0.0",
    "commitizen": "^2.10.1",
    "concurrently": "3.6.1",
    "cross-env": "^5.2.0",
    "cz-customizable": "^5.5.3",
    "eslint": "^5.15.3",
    "jest": "^24.5.0",
    "jest-circus": "^24.5.0",
    "jest-watch-select-projects": "^0.1.1",
    "lerna": "^3.13.1",
    "rimraf": "^2.6.1",
    "rollup": "^1.7.0",
    "rollup-plugin-babel": "^4.3.2",
    "rollup-plugin-commonjs": "^9.2.1",
    "rollup-plugin-node-resolve": "^4.0.1",
    "ts-jest": "^24.0.0",
    "typescript": "^3.3.3333"
  },
  "scripts": {
    "clean:modules": "rimraf node_modules/*",
    "canary": "lerna publish --canary",
    "lerna": "lerna",
    "tsc": "yarn run lerna run tsc",
    "commit:generators": "git add packages\\styleaux-generators && yarn commit",
    "commit:core": "git add packages\\styleaux-core && yarn commit",
    "commit:coreTest": "git add packages/styleaux-core/src/core/__tests__ && yarn commit",
    "commit:basestyles": "git add packages\\styleaux-styles-base && yarn commit",
    "commit:all": "git add . && yarn commit",
    "add": "lerna add",
    "boot": "lerna bootstrap",
    "clean": "lerna clean",
    "ls": "yarn run lerna ls",
    "link": "yarn run lerna link",
    "build": "yarn run lerna run build",
    "cleanup": "yarn run clean && rm -rf node_modules/ && npm install && npm run boot",
    "lint": "eslint . --ignore-path .eslintignore --cache",
    "test": "jest",
    "commit": "git cz"
  },
  "workspaces": [
    "packages/*"
  ],
  "lint-staged": {
    "*.js": [
      "prettier --no-semi --single-quote --write",
      "git add"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "proseWrap": "never",
    "singleQuote": true,
    "trailingComma": "all",
    "semi": false
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-customizable"
    }
  },
  "dependencies": {},
  "version": "0.0.0"
}
